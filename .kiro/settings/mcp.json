{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"],
      "env": {},
      "disabled": false,
      "autoApprove": ["git_status", "git_log", "git_diff"]
    },
    "filesystem": {
      "command": "uvx",
      "args": ["mcp-server-filesystem", "--allowed-directories", "."],
      "env": {},
      "disabled": false,
      "autoApprove": ["read_file", "list_directory", "search_files"]
    }
  }
}
